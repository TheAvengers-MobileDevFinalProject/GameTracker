<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="edu.ualr.gametrackerproject">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_new_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_new_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="edu.ualr.gametrackerproject.ListHomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="edu.ualr.gametrackerproject.ListInfoActivity" />
        <activity android:name="edu.ualr.gametrackerproject.GameChoiceActivity" />
        <activity android:name="edu.ualr.gametrackerproject.ListEntryActivity" />
        <activity android:name="edu.ualr.gametrackerproject.PSFormActivity" />
        <activity android:name="edu.ualr.gametrackerproject.XBFormActivity"/>
        <activity android:name="edu.ualr.gametrackerproject.NTFormActivity" />

    </application>

</manifest>